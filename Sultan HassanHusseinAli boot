// كود الحماية وObfuscation للزوار
(function(){
  function o(s){return s.split("").reverse().join("");}
  document.addEventListener("contextmenu",function(e){e.preventDefault();});
  document.addEventListener("keydown",function(e){
    if(
      e.key=="F12"||
      (e.ctrlKey&&(e.key=="u"||e.key=="s"||e.key=="c"||e.key=="a"||e.key=="p"))||
      (e.metaKey&&(e.key=="u"||e.key=="s"||e.key=="c"))
    ) e.preventDefault();
  });
  document.addEventListener("dragstart",e=>e.preventDefault());
  document.addEventListener("selectstart",e=>e.preventDefault());
  setInterval(function(){
    if(window.outerWidth - window.innerWidth > 160 ||
       window.outerHeight - window.innerHeight > 160){
      document.body.innerHTML = "<h2 style='color:red'>🚫 تم حظر الصفحة لحماية المحتوى</h2>";
    }
  }, 1000);
  setTimeout(function(){
    for(let i=0;i<5;i++)console.log(o("🔥 يرحب بكم Sultan Hussein bot 🔥 | محمي بالكامل - يمنع الفتح البرمجي 🔥"));
  },333);
})();
function updateCounters(id, step) {
  let val = localStorage.getItem(id) || document.getElementById(id).textContent;
  val = parseInt(val) + step;
  localStorage.setItem(id, val);
  document.getElementById(id).textContent = val;
}
setInterval(()=>{
  let now = new Date();
  let options = {hour:'2-digit',minute:'2-digit',second:'2-digit',hour12:false,timeZone:'Africa/Cairo'};
  document.getElementById("clock").textContent = "التوقيت (القاهرة +24): " + now.toLocaleTimeString('ar-EG',options);
  document.getElementById("date").textContent = "التاريخ الميلادي: " + now.toLocaleDateString('ar-EG',{timeZone:'Africa/Cairo'});
  let hijri = new Intl.DateTimeFormat('ar-TN-u-ca-islamic',{day:'numeric',month:'long',year:'numeric'}).format(now);
  document.getElementById("hijri").textContent = "التاريخ الهجري: " + hijri;
  updateCounters("followers", Math.floor(Math.random()*10)+1);
  updateCounters("likes", Math.floor(Math.random()*5)+1);
  updateCounters("comments", Math.floor(Math.random()*2)+1);
  updateCounters("visitors", Math.floor(Math.random()*3)+1);
},1000);
</script>
