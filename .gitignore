window.onload = function () {
  const voice = document.getElementById("welcome-voice");
  const music = document.getElementById("background-music");

  // شغل الصوت الترحيبي الأول
  voice.play();

  // بعد انتهاء الصوت، شغل الموسيقى
  voice.onended = () => {
    music.play();
  };
};
